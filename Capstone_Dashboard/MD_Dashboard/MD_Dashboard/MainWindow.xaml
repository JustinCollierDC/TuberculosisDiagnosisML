<Window x:Class="MD_Dashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MD_Dashboard"
        mc:Ignorable="d"
        Title="Lakeridge Dashboard Prototype" Height="400" Width="295">
    <Grid>
        <Button x:Name="btnExit" Content="_Exit" HorizontalAlignment="Left" Margin="100,289,0,0" VerticalAlignment="Top" Height="53" Width="168" Click="btnExit_Click"/>
        <Button x:Name="btnPatient" Content="_Open Patient Form" HorizontalAlignment="Left" Margin="100,210,0,0" VerticalAlignment="Top" Height="53" Width="168" Click="btnPatientClick"/>

        <Image x:Name="imgRoomUtil" HorizontalAlignment="Left" Height="52" VerticalAlignment="Top" Width="61" Source="./RoomUtil.png" Margin="32,208,0,0">
            <Image.Effect>
                <DropShadowEffect/>
            </Image.Effect>
        </Image>

        <Image x:Name="imgExit" HorizontalAlignment="Left" Height="52" VerticalAlignment="Top" Width="61" Source="./RoomUtil.png" Margin="32,285,0,0">
            <Image.Effect>
                <DropShadowEffect/>
            </Image.Effect>
        </Image>
        <Image Margin="21,10,25,226" Source="./Lakeridge-Health.jpg" Stretch="Fill">

        </Image>

    </Grid>
</Window>
